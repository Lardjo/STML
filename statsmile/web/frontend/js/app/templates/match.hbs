<div class="page-header" id="match">
  <div class="row">
    <div class="col-xs-6 col-md-6">
      <h2>Match Info <small>#{{id}}</small></h2>
    </div>
      <div class="col-xs-6 col-md-6">
        {{#if radiant_win}}<h2 class="text-right text-success">Radiant Victory</h2>
        {{else}}<h2 class="text-right text-danger">Dire Victory</h2>{{/if}}
      </div>
  </div>
</div>
<h4>GENERAL INFORMATION</h4>
<div class="status-panel">
  <div class="top">
    <span class="batch batch-huge uniF160"></span>
    <h6>Match ID</h6>
  </div>
  <div class="bottom">
    <h2>{{id}}</h2>
    <h6>Unique ID of the match</h6>
  </div>
</div>
<div class="status-panel">
  <div class="top">
    <span class="batch batch-huge uniF05B"></span>
    <h6>Start Date</h6>
  </div>
  <div class="bottom">
    <h2>{{date-full start_time}}</h2>
    <h6>{{date-short start_time}} GMT</h6>
  </div>
</div>
<div class="status-panel">
  <div class="top">
    <span class="batch batch-huge uniF166"></span>
    <h6>Duration</h6>
  </div>
  <div class="bottom">
    <h2>{{date-short duration}}</h2>
    <h6>Time of match</h6>
  </div>
</div>
<div class="status-panel">
  <div class="top">
    <span class="batch batch-huge uniF028"></span>
    <h6>Game Mode</h6>
  </div>
  <div class="bottom">
    <h2>{{mode game_mode}}</h2>
    <h6>{{lobby lobby_type}}</h6>
  </div>
</div>
<div class="status-panel">
  <div class="top">
    <span class="batch batch-huge uniF07C"></span>
    <h6>Server</h6>
  </div>
  <div class="bottom">
    <h2>{{server cluster}}</h2>
    <h6>Regional Cluster</h6>
  </div>
</div>
<div class="status-panel">
  <div class="top">
    <span class="batch batch-huge uniF12E"></span>
    <h6>First Blood</h6>
  </div>
  <div class="bottom">
    <h2>{{date-short first_blood_time}}</h2>
    <h6>Time Of First Death</h6>
  </div>
</div>
<h4>FINAL RESULTS</h4>
<div class="panel panel-success" id="match-result">
  <div class="panel-heading">The Radiant Team</div>
    <table class="table">
      <thead>
        <tr>
          <th>Player</th>
          <th>Hero</th>
          <th>Items</th>
          <th>Level</th>
          <th>Gold</th>
          <th>LH</th>
          <th>DN</th>
          <th>HD</th>
          <th>XPM</th>
          <th>GPM</th>
          <th>Kills / Deaths / Assists</th>
        </tr>
      </thead>
      <tbody>
        {{#each player in players}}
        {{#if player.radiant}}
        <tr>
          <td class="player-name text-muted">Anonymous</td>
          <td class="hero-cont-pad">
            {{{player.hero_id}}}
          </td>
          <td class="item-cont-pad">
            {{#each item in player.items}}
            <div class="item-cont">
              {{{item}}}
            </div>
            {{/each}}
          </td>
          <td>{{player.level}}</td>
          <td>{{player.gold}}</td>
          <td>{{player.last_hits}}</td>
          <td>{{player.denies}}</td>
          <td>{{player.hero_damage}}</td>
          <td>{{player.xp_per_min}}</td>
          <td>{{player.gold_per_min}}</td>
          <td>
            <div class="progress">
              {{{player.kills}}}
              {{{player.deaths}}}
              {{{player.assists}}}
            </div>
          </td>
        </tr>
        {{/if}}
        {{/each}}
      </tbody>
  </table>
</div>
<div class="panel panel-danger" id="match-result">
  <div class="panel-heading">The Dire Team</div>
    <table class="table">
      <thead>
        <tr>
          <th>Player</th>
          <th>Hero</th>
          <th>Items</th>
          <th>Level</th>
          <th>Gold</th>
          <th>LH</th>
          <th>DN</th>
          <th>HD</th>
          <th>XPM</th>
          <th>GPM</th>
          <th>Kills / Deaths / Assists</th>
        </tr>
      </thead>
      <tbody>
        {{#each player in players}}
        {{#if player.radiant}}
        {{else}}
        <tr>
          <td class="player-name text-muted">Anonymous</td>
          <td class="hero-cont-pad">
            {{{player.hero_id}}}
          </td>
          <td class="item-cont-pad">
            {{#each item in player.items}}
            <div class="item-cont">
              {{{item}}}
            </div>
            {{/each}}
          </td>
          <td>{{player.level}}</td>
          <td>{{player.gold}}</td>
          <td>{{player.last_hits}}</td>
          <td>{{player.denies}}</td>
          <td>{{player.hero_damage}}</td>
          <td>{{player.xp_per_min}}</td>
          <td>{{player.gold_per_min}}</td>
          <td>
            <div class="progress">
              {{{player.kills}}}
              {{{player.deaths}}}
              {{{player.assists}}}
            </div>
          </td>
        </tr>
        {{/if}}
        {{/each}}
      </tbody>
  </table>
</div>